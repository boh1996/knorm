{
  "name": "knex-orm",
  "version": "0.0.1",
  "description": "A purely class-based ORM for Knex.js",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "coverage": "nyc mocha",
    "lint": "eslint .",
    "ci": "npm-run-all lint coverage",
    "postgres": "docker run --name knex-orm-postgres -p 5432:5432 -d postgres",
    "postgres:stop": "docker rm -f knex-orm-postgres"
  },
  "author": "Joel Mukuthu <joelmukuthu@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "knex": "^0.12.7"
  },
  "devDependencies": {
    "eslint": "^3.17.1",
    "eslint-config-onelint": "^2.0.0",
    "knex": "^0.12.7",
    "mocha": "^3.2.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^10.1.2",
    "pg": "^6.1.4",
    "proxyquire": "^1.7.11",
    "sinon": "^1.17.7",
    "unexpected": "^10.26.3",
    "unexpected-sinon": "^10.7.0"
  },
  "directories": {
    "test": "test"
  },
  "keywords": [
    "knex",
    "orm",
    "erm",
    "database"
  ],
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "nyc": {
    "all": true,
    "cache": true,
    "include": [ "*.js", "lib/**/*.js" ],
    "exclude": [ "index.js" ]
  }
}
